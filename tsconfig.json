{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "allowJs": true,
    "checkJs": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "moduleResolution": "node",
    "module": "esnext", // <-- thêm dòng này
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["app"]
}
