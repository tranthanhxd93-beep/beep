{
  "expo": {
    "name": "Quản Lý Trại Dúi",
    "slug": "quan-ly-trai-dui",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trai.quanlytraidui"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.trai.quanlytraidui"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "23526338-1798-4a14-9c96-b9fbab3e8a06"
      }
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ]
  }
}
